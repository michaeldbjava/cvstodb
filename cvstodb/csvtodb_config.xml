<csvtodb>
	<csvfile></csvfile>
	<database>
		<!-- 
			dbtypes: mysql, mssql, postgrsql 
			datbase: select database of database server
			port: port number of database server
			user: user of database
			password: password for user of database	
		-->
		<dbtype></dbtype>
		<database></database>
		<port></port>
		<user></user>
		<password></password>
	</database>
	<!--
		Path to csv file, to import to database 
	 -->
	<file-to-db-table></file-to-db-table>
	<!-- Mapping from csv fields to table columns -->
	<map-csv-to-db>
		<field-mapping>
			<csv-field></csv-field>
			<table-column></table-column>
		</field-mapping>
		<field-mapping>
			<csv-field></csv-field>
			<table-column></table-column>
		</field-mapping>
		<field-mapping>
			<csv-field></csv-field>
			<table-column></table-column>
		</field-mapping>
	</map-csv-to-db>
</csvtodb>